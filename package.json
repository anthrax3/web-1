{
  "name": "@emmetio/web",
  "version": "0.0.1",
  "description": "A web-site for Emmet 2.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:html && npm run build:css && npm run build:app",
    "build:app": "rollup -c",
    "build:html": "cpx ./src/index.html ./out/",
    "build:css": "postcss ./src/style/*.css -m -d ./out/style",
    "watch": "npm run build:html -- -w & npm run build:css -- -w & npm run build:app -- -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Sergey Chikuyonok <serge.che@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "buble": "^0.19.3",
    "cpx": "^1.5.0",
    "eslint-plugin-html": "^4.0.2",
    "postcss": "^6.0.19",
    "postcss-cli": "^5.0.0",
    "postcss-import": "^11.1.0",
    "rollup": "^0.56.3",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-svelte": "^4.0.0",
    "rollup-watch": "^4.3.1",
    "svelte": "^1.56.0"
  },
  "dependencies": {
    "@emmetio/codemirror-plugin": "^0.4.2",
    "@emmetio/expand-abbreviation": "^0.5.10",
    "codemirror": "^5.35.0"
  }
}
